@model IEnumerable<MVC_Blog.Models.Kategori>
<!-- Categories Widget --><div class="well">
    <div class="card my-4">
        <h5 class="card-header">Kategoriler</h5>
        <div class="card-body">
            <div class="row">
                
                    @foreach (var item in Model)
                    {
                <div class="col-lg-6">
                    <a href="/Blog/List/@item.Id">@item.KategoriAd</a>

                </div>
}
        </div>
    </div>
</div></div>
